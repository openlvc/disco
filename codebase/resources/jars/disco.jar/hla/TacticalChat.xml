<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<objectModel xsi:schemaLocation="http://standards.ieee.org/IEEE1516-2010 http://standards.ieee.org/downloads/1516/1516.2-2010/IEEE1516-DIF-2010.xsd" xmlns="http://standards.ieee.org/IEEE1516-2010" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <modelIdentification>
        <name>New Module</name>
        <type>FOM</type>
        <version>1.0</version>
        <securityClassification>unclassified</securityClassification>
        <purpose></purpose>
        <applicationDomain></applicationDomain>
        <description>Description of New Module</description>
        <useLimitation></useLimitation>
        <poc>
            <pocType>Maintainer</pocType>
            <pocName>CNR Engineering Team</pocName>
            <pocOrg>Calytrix Technologies</pocOrg>
            <pocEmail>support@calytrix.com</pocEmail>
        </poc>
        <poc>
            <pocType></pocType>
            <pocName></pocName>
            <pocOrg></pocOrg>
        </poc>
        <other></other>
    </modelIdentification>
    <!--  ======================================================================= -->
    <!--                         Object Class Definitions                         -->
    <!--  ======================================================================= -->
    <objects>
        <objectClass>
            <name>HLAobjectRoot</name>
            <objectClass>
                <name>IRCUser</name>
                <sharing>Subscribe</sharing>
                <semantics>Represents core data held in an IRC Server.</semantics>
                <attribute>
                    <name>UserId</name>
                    <dataType>EntityIdentifierStruct</dataType>
                    <updateType>Static</updateType>
                    <updateCondition>NA</updateCondition>
                    <ownership>DivestAcquire</ownership>
                    <sharing>PublishSubscribe</sharing>
                    <transportation>HLAbestEffort</transportation>
                    <order>Receive</order>
                    <semantics>Unique ID for this IRC User.</semantics>
                </attribute>
                <attribute>
                    <name>UserNick</name>
                    <dataType>HLAASCIIstring</dataType>
                    <updateType>On change</updateType>
                    <updateCondition>NA</updateCondition>
                    <ownership>DivestAcquire</ownership>
                    <sharing>PublishSubscribe</sharing>
                    <transportation>HLAbestEffort</transportation>
                    <order>Receive</order>
                    <semantics>IRC Nickname for the user.</semantics>
                </attribute>
                <attribute>
                    <name>Channels</name>
                    <dataType>IRCChannelArray</dataType>
                    <updateType>On change</updateType>
                    <updateCondition>NA</updateCondition>
                    <ownership>DivestAcquire</ownership>
                    <sharing>PublishSubscribe</sharing>
                    <transportation>HLAbestEffort</transportation>
                    <order>Receive</order>
                    <semantics>List of channels that are active in the server.</semantics>
                </attribute>
            </objectClass>
        </objectClass>
    </objects>


    <!--  ======================================================================= -->
    <!--                       Interaction Class Definitions                      -->
    <!--  ======================================================================= -->    
    <interactions>
        <interactionClass>
            <name>HLAinteractionRoot</name>
            <!-- Class: IRCChannelMessage -->
            <interactionClass>
                <name>IRCChannelMessage</name>
                <sharing>PublishSubscribe</sharing>
                <transportation>HLAreliable</transportation>
                <order>Receive</order>
                <semantics></semantics>
                <parameter>
                    <name>ChannelName</name>
                    <dataType>HLAASCIIstring</dataType>
                    <semantics>The name of the IRC channel the message was sent in</semantics>
                </parameter>
                <parameter>
                    <name>SenderId</name>
                    <dataType>EntityIdentifierStruct</dataType>
                    <semantics>The unique id of the message sender</semantics>
                </parameter>
                <parameter>
                    <name>SenderNick</name>
                    <dataType>HLAASCIIstring</dataType>
                    <semantics>The IRC nick of the message sender</semantics>
                </parameter>
                <parameter>
                    <name>Message</name>
                    <dataType>HLAASCIIstring</dataType>
                    <semantics>The IRC message</semantics>
                </parameter>
                <parameter>
                    <name>TimeSent</name>
                    <dataType>UnsignedInteger64</dataType>
                    <semantics>Epoch timestamp sent at message transimission</semantics>
                </parameter>
            </interactionClass>

            <!-- Class: IRCRawMessage -->
            <interactionClass>
                <name>IRCRawMessage</name>
                <sharing>PublishSubscribe</sharing>
                <transportation>HLAreliable</transportation>
                <order>Receive</order>
                <semantics></semantics>
                <parameter>
                    <name>Prefix</name>
                    <dataType>HLAASCIIstring</dataType>
                    <semantics></semantics>
                </parameter>
                <parameter>
                    <name>Command</name>
                    <dataType>HLAASCIIstring</dataType>
                    <semantics></semantics>
                </parameter>
                <parameter>
                    <name>CommandParameters</name>
                    <dataType>HLAASCIIstring</dataType>
                    <semantics></semantics>
                </parameter>
                <parameter>
                    <name>TimeReceived</name>
                    <dataType>UnsignedInteger64</dataType>
                    <semantics>Epoch timestamp sent at message transimission</semantics>
                </parameter>
                <parameter>
                    <name>SenderId</name>
                    <dataType>EntityIdentifierStruct</dataType>
                    <semantics>ServerId unique id for the sender</semantics>
                </parameter>
                <parameter>
                    <name>SenderNick</name>
                    <dataType>HLAASCIIstring</dataType>
                    <semantics>The IRC nick of the sender</semantics>
                </parameter>
            </interactionClass>
        </interactionClass>
    </interactions>
    <!--  ======================================================================= -->
    <!--                           Data Type Definitions                          -->
    <!--  ======================================================================= -->
    <dataTypes>
        <simpleDataTypes/>
        <enumeratedDataTypes/>
        <variantRecordDataTypes/>

        <!-- ==================== -->
        <!--    Array Datatypes   -->
        <!-- ==================== -->
        <arrayDataTypes>
            <arrayData>
                <name>IRCChannelArray</name>
                <dataType>HLAASCIIString</dataType>
                <cardinality>Dynamic</cardinality>
                <encoding>HLAvariableArray</encoding>
                <semantics>List of channel names on an IRC network.</semantics>
            </arrayData>
        </arrayDataTypes>

    </dataTypes>
    <dataTypes>
        <arrayDataTypes/>
        <fixedRecordDataTypes/>
    </dataTypes>
</objectModel>
