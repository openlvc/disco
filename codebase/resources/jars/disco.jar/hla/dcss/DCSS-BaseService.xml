<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<objectModel xsi:schemaLocation="http://standards.ieee.org/IEEE1516-2010 http://standards.ieee.org/downloads/1516/1516.2-2010/IEEE1516-DIF-2010.xsd" xmlns="http://standards.ieee.org/IEEE1516-2010" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <modelIdentification>
        <name>DCS&amp;S Base Service</name>
        <type>FOM</type>
        <version>1.1</version>
        <modificationDate>2019-01-03</modificationDate>
        <securityClassification>unclassified</securityClassification>
        <purpose></purpose>
        <applicationDomain></applicationDomain>
        <description>Description of New Module</description>
        <useLimitation></useLimitation>
        <useHistory>Version 1.1 - Added a DCS&amp;S base object class</useHistory>
        <poc>
            <pocType>Primary author</pocType>
            <pocName>Michael Jobson</pocName>
            <pocOrg>Boeing Defence UK</pocOrg>
            <pocEmail>michael.i.jobson@boeing.com</pocEmail>
        </poc>
        <poc>
            <pocType>Contributor</pocType>
            <pocName>Alan Wills</pocName>
            <pocOrg>Boeing Defence UK</pocOrg>
            <pocEmail>alan.w.wills@boeing.com</pocEmail>
        </poc>
        <other>DRAFT</other>
    </modelIdentification>
    <objects>
        <objectClass>
            <name>HLAobjectRoot</name>
            <objectClass>
                <name>DCSSBaseObject</name>
                <sharing>Neither</sharing>
                <semantics>Base object for all derived DCSS object classes</semantics>
            </objectClass>
        </objectClass>
    </objects>
    <interactions>
        <interactionClass>
            <name>HLAinteractionRoot</name>
            <interactionClass>
                <name>Service</name>
                <sharing>PublishSubscribe</sharing>
                <transportation>HLAreliable</transportation>
                <order>Receive</order>
                <semantics>Scaffolding class to denote a service interaction</semantics>
                <interactionClass>
                    <name>Message</name>
                    <sharing>PublishSubscribe</sharing>
                    <transportation>HLAreliable</transportation>
                    <order>Receive</order>
                    <semantics>General purpose message interaction with a free text attribute and a variable datum set array</semantics>
                    <parameter>
                        <name>Id</name>
                        <dataType>UuidArrayOfHLAbyte16</dataType>
                        <semantics>Unique id for this error</semantics>
                    </parameter>
                    <parameter>
                        <name>FederateName</name>
                        <dataType>HLAASCIIstring</dataType>
                        <semantics>Name of federate that generated the error.</semantics>
                    </parameter>
                    <parameter>
                        <name>Text</name>
                        <dataType>HLAASCIIstring</dataType>
                        <semantics>The message text.</semantics>
                    </parameter>
                    <parameter>
                        <name>Timestamp</name>
                        <dataType>DateTimeStruct</dataType>
                        <semantics>Time of the error</semantics>
                    </parameter>
                    <parameter>
                        <name>Level</name>
                        <dataType>LevelType</dataType>
                        <semantics>The level of the message</semantics>
                    </parameter>
                </interactionClass>
            </interactionClass>
        </interactionClass>
    </interactions>
    <dataTypes>
        <simpleDataTypes/>
        <enumeratedDataTypes>
            <enumeratedData>
                <name>LevelType</name>
                <representation>HLAoctet</representation>
                <semantics>The log level of the associated message</semantics>
                <enumerator>
                    <name>DEBUG</name>
                    <value>0</value>
                </enumerator>
                <enumerator>
                    <name>INFO</name>
                    <value>1</value>
                </enumerator>
                <enumerator>
                    <name>WARNING</name>
                    <value>2</value>
                </enumerator>
                <enumerator>
                    <name>SEVERE</name>
                    <value>3</value>
                </enumerator>
                <enumerator>
                    <name>CRITICAL</name>
                    <value>4</value>
                </enumerator>
            </enumeratedData>
        </enumeratedDataTypes>
        <arrayDataTypes/>
        <fixedRecordDataTypes>
            <fixedRecordData>
                <name>DateTimeStruct</name>
                <encoding>HLAfixedRecord</encoding>
                <semantics>A fixed record to start the date/time stamp</semantics>
                <field>
                    <name>Year</name>
                    <dataType>Integer16</dataType>
                    <semantics>Year value</semantics>
                </field>
                <field>
                    <name>Month</name>
                    <dataType>Integer16</dataType>
                    <semantics>Month value</semantics>
                </field>
                <field>
                    <name>Day</name>
                    <dataType>Integer16</dataType>
                    <semantics>Day Value</semantics>
                </field>
                <field>
                    <name>Hour</name>
                    <dataType>Integer16</dataType>
                    <semantics>Hour value (in 24 hour notation)</semantics>
                </field>
                <field>
                    <name>Minute</name>
                    <dataType>Integer16</dataType>
                    <semantics>Minute value</semantics>
                </field>
                <field>
                    <name>Second</name>
                    <dataType>Integer16</dataType>
                    <semantics>Second value</semantics>
                </field>
            </fixedRecordData>
        </fixedRecordDataTypes>
        <variantRecordDataTypes/>
    </dataTypes>
</objectModel>
